; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Lucky Deck"
run/main_scene="res://scenes/LoginScene.tscn"
config/features=PackedStringArray("4.3", "Mobile")

[autoload]

ConfigManager="*res://scripts/ConfigManager.gd"
ErrorManager="*res://scripts/ErrorManager.gd"
CardLoader="*res://scripts/CardLoader.gd"
AnimationManager="*res://scripts/AnimationManager.gd"
GameData="*res://scripts/GameData.gd"
SceneManager="*res://scripts/SceneManager.gd"
Utilities="*res://scripts/Utilities.gd"
User="*res://scripts/User.gd"
APIManager="*res://scripts/APIManager.gd"
PlayerData="*res://scripts/PlayerData.gd"
TableManager="*res://scripts/TableManager.gd"
GameJoiner="*res://scripts/GameJoiner.gd"
SportsBettingManager="*res://scripts/SportsBettingManager.gd"
BetManager="*res://scripts/BetManager.gd"
BlackjackManager="*res://scripts/BlackjackManager.gd"
JackpotSNGManager="*res://scripts/JackpotSNGManager.gd"
ChallengesManager="*res://scripts/challenges/ChallengesManager.gd"

[display]

window/size/viewport_width=1900
window/size/viewport_height=1020
window/size/initial_position_type=0
window/stretch/mode="viewport"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
viewport/hdr_2d=true
